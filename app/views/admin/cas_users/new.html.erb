<%= form_for(@user, url: admin_cas_users_path, html: { class: "form new_user" }) do |f| %>
  <div class="card">
    <div class="card-divider">
      <h2 class="card-title">Crea una usuari al Decidim</h2>
    </div>
    <div class="card-section">
      <div><%= f.text_field :name, label: "Nom", value: nil %></div>
      <div><%= f.text_field :email %></div>
      <div>
        <%= f.label "Numero de soci" %>
        <%= text_field_tag "soci", nil, type: "number" %>
      </div>
      <div>
        <%= f.label "DNI" %>
        <%= text_field_tag "dni", nil %>
      </div>
    </div>
  </div>

  <div class="button--double form-general-submit">
    <%= f.submit "Crea-la" %>
  </div>
<% end %>
